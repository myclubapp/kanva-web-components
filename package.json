{
  "name": "kanva-web-components",
  "version": "1.5.0",
  "description": "KANVA - Das Social Media Tool f√ºr Sportvereine",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/kanva-web-components/kanva-web-components.esm.js",
  "keywords": [
    "social-media",
    "sport",
    "kanva",
    "whatsapp",
    "instagram"
  ],
  "homepage": "https://getkanva.io",
  "author": "KANVA",
  "exports": {
    ".": {
      "import": "./dist/kanva-web-components/kanva-web-components.esm.js",
      "require": "./dist/kanva-web-components/kanva-web-components.cjs.js"
    },
    "./game-preview": {
      "import": "./dist/components/game-preview.js",
      "types": "./dist/components/game-preview.d.ts"
    },
    "./game-result": {
      "import": "./dist/components/game-result.js",
      "types": "./dist/components/game-result.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/myclubapp/kanva-web-components.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.38.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.10",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "24.4.0"
  },
  "license": "MIT"
}
